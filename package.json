{
  "name": "unslackd",
  "version": "1.0.0",
  "description": "Bring Untappd info to Slack",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf build && mkdir build",
    "build-babel": "babel -d ./build ./src -s",
    "build": "npm run clean && npm run build-babel",
    "start": "npm run build && node ./build/index.js",
    "dev": "npm run build && node -r dotenv/config ./build/index.js",
    "test": "mocha --require babel-polyfill --require babel-register --bail --recursive --sort --full-trace ./test"
  },
  "dependencies": {
    "babel-preset-env": "^1.6.1",
    "bluebird": "^3.5.1",
    "body-parser": "1.18.2",
    "dotenv": "^5.0.1",
    "express": "4.16.3",
    "request": "^2.85.0",
    "request-promise": "^4.2.2"
  },
  "engines": {
    "node": "6.10.3"
  },
  "author": "Harry Paterra",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.9.0",
    "mocha": "^5.0.5"
  }
}
