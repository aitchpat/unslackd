{
  "name": "unslackd",
  "version": "1.0.0",
  "description": "Bring Untappd info to Slack",
  "main": "index.js",
  "scripts": {
    "start": "npm run serve",
    "build": "npm run build-ts && npm run tslint",
    "serve": "node build/index.js",
    "dev": "node -r dotenv/config ./build/index.js",
    "build-ts": "tsc",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "test": "npm run build && mocha --bail --recursive --sort --full-trace ./build/test"
  },
  "dependencies": {
    "@slack/interactive-messages": "^0.2.0",
    "babel-cli": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "dotenv": "^5.0.1",
    "express": "^4.16.4",
    "request": "^2.88.0",
    "request-promise": "^4.2.2"
  },
  "author": "Harry Paterra",
  "license": "MIT",
  "devDependencies": {
    "@types/bluebird": "^3.5.24",
    "@types/chai": "^4.1.5",
    "@types/dotenv": "^4.0.3",
    "@types/express": "^4.16.0",
    "@types/mocha": "^5.2.5",
    "@types/request-promise": "^4.1.42",
    "chai": "^4.1.2",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.10.0",
    "mocha": "^5.0.5",
    "tslint": "^5.11.0",
    "typescript": "^3.1.1"
  }
}
