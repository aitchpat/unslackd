{
  "name": "unslackd",
  "version": "1.0.0",
  "author": "Harry Paterra",
  "license": "MIT",
  "description": "Bring Untappd info to Slack",
  "main": "dist/index.js",
  "engines": {
    "node": "16.14.0"
  },
  "scripts": {
    "clean": "rm -rf build && mkdir build",
    "build-tsc": "tsc",
    "build": "npm run clean && npm run build-tsc",
    "start": "npm run build && node ./build/index.js",
    "dev": "npm run build && node -r dotenv/config ./build/index.js",
    "test": "mocha --require ts-node/register --extension test.ts --bail --recursive --sort --full-trace ./test",
    "debug": "npm run build && node --inspect-brk -r dotenv/config ./build/index.js"
  },
  "dependencies": {
    "@slack/interactive-messages": "^2.0.2",
    "body-parser": "^1.19.2",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "module-alias": "^2.2.2",
    "node-fetch": "~2.6.7"
  },
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.1.0",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^17.0.21",
    "@types/node-fetch": "^2.6.1",
    "chai": "^4.3.6",
    "eslint": "^8.10.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4",
    "mocha": "^9.2.1",
    "ts-node": "^10.6.0",
    "typescript": "^4.6.2"
  },
  "_moduleAliases": {
    "~": "build"
  }
}
